<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Platforms>AnyCPU</Platforms>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>1.0.0</Version>
    <RepositoryUrl>https://github.com/islero/High-Performance-Backtest.Net</RepositoryUrl>
    <Description>Fixed DaysPerSplit &lt;= 0</Description>
    <PackageReleaseNotes>Implemented Backtesting Executor, release candidate</PackageReleaseNotes>
    <Configurations>Release;Debug</Configurations>
  </PropertyGroup>

</Project>
