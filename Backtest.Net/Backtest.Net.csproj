<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Platforms>AnyCPU</Platforms>
    <AssemblyVersion>3.0.12.0</AssemblyVersion>
    <FileVersion>3.0.12.0</FileVersion>
    <Version>3.0.12</Version>
    <RepositoryUrl>https://github.com/islero/High-Performance-Backtest.Net</RepositoryUrl>
    <Description></Description>
    <PackageReleaseNotes>Partially fixed bot progress for engines starting from V2</PackageReleaseNotes>
    <Configurations>Release;Debug</Configurations>
    <Authors>Yurii Dumanskyi</Authors>
    <Title>High-Performance Backtest.Net library</Title>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="models.net" Version="1.0.20" />
    <PackageReference Include="SimdLinq" Version="1.3.2" />
  </ItemGroup>

</Project>
